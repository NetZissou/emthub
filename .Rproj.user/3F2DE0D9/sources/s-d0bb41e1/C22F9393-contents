addr <- "LAKE MILTON PRESBYTERIAN CHR, 942 GRANDVIEW RD, LAKE MILTON, 44429"
encoded_address <- urltools::url_encode(addr)
google_maps_url <- paste0("https://www.google.com/maps/search/?api=1&query=", encoded_address)

